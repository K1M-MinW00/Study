// 1.2 배열의 수 더하기 ( 수가 n 개인 배열 S 에 있는 수를 모두 더하시오. )
// Input : 정수 n (> 0) , 수의 배열 S (인덱스 범위는 1 ~ n)
// Output : S 에 있는 모든 수의 합
#include <iostream>
#include <vector>
using namespace std;

int sum(int n, vector<int> &S)
{
    int i, result = 0;
    for (i = 1; i <= n; i++)
        result += S[i];

    return result;
}
